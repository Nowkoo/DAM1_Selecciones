create table pais(
id_pais numeric(2),
nombre_pais varchar(50),
constraint pk_pais primary key (id_pais)
);

create table club(
id_club numeric(2),
nombre_club varchar(50),
constraint pk_clib primary key (id_club)
);

create table jugador(
id_jugador numeric(5),
nombre_jugador varchar(50),
aÃ±o_nacimiento date,
altura numeric(1.2),
id_pais numeric(2),
id_club numeric(2),
constraint pk_pais primary key (id_pais),
constraint fk_jugador_pais foreign key (id_pais) references pais (id_pais),
constraint fk_jugador_club foreign key (id_club) references club (id_club)
);

